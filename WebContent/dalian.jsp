            <script>

                Vue.filter('dateFormat', function (dateStr, pattern = "") {
                    // 根据给定的时间字符串，得到特定的时间
                    var dt = new Date(dateStr)

                    //   yyyy-mm-dd
                    var y = dt.getFullYear()
                    var m = dt.getMonth() + 1
                    var d = dt.getDate()

                    // return y + '-' + m + '-' + d



                    if (pattern.toLowerCase() === 'yyyy-mm-dd') {
                        return `${y}-${m}-${d}`
                    } else {
                        var hh = dt.getHours()
                        var mm = dt.getMinutes()
                        var ss = dt.getSeconds()

                        return `${y}-${m}-${d} ${hh}:${mm}:${ss}`
                    }
                })


                var vm = new Vue({
                    el: '#app',
                    data: {
                        id:'9',
                        name: '',
                        clazzid: '',
                        clazzname:'',

                        keywords: '', // 搜索的关键字
                        list: [
                            { id: 1, name: '罗丽娟',clazzid:'1600106',clazzname:'软件技术与开发',ctime: new Date() },
                            { id: 2, name: '陈斌',clazzid:'1600106',clazzname:'Java高级框架与应用', ctime: new Date() },
                            { id: 3, name: '石磊',clazzid:'1600106',clazzname:'JavaScript', ctime: new Date() },
                            { id: 4, name: '陈凌',clazzid:'1600106',clazzname:'ssm框架', ctime: new Date() },
                            { id: 5, name: '宋宇',clazzid:'1600106',clazzname:'Java基础', ctime: new Date() },
                            { id: 6, name: '何春梅',clazzid:'1600106',clazzname:'软件测试', ctime: new Date() },
                            { id: 7, name: '冷压红',clazzid:'1600106',clazzname:'Uml', ctime: new Date() },
                            { id: 8, name: '张正龙',clazzid:'1600106',clazzname:'Javaweb', ctime: new Date() },

                        ]
                    },
                    methods: {

                        add(id) {

                            var car = { id: this.id, name: this.name,clazzid: this.clazzid,clazzname:this.clazzname, ctime: new Date() }

                            this.list.push(car)
                            this.clazzname = this.clazzid=this.name=''
                            console.log(this.id)
                            this.id++
                        },
                        del(id) {


                            var index = this.list.findIndex(item => {
                                if (item.id == id) {
                                return true;
                            }
                        })

                            // console.log(index)
                            this.list.splice(index, 1)
                        },
                        search(keywords) { //0 根据关键字，进行数据的搜索

                            return this.list.filter(item => {

                                if (item.name.includes(keywords)) {
                                return item
                            }
                        })

                        }
                    }
                });




            </script>
